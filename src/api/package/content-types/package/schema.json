{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "integer"
    },
    "start": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "status_package": {
      "type": "enumeration",
      "enum": [
        "Draft",
        "Publish"
      ]
    },
    "stripe_product_id": {
      "type": "string"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "package"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location",
      "mappedBy": "package"
    }
  }
}
