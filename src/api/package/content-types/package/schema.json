{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "package_id": {
      "type": "integer",
      "unique": true
    },
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "integer"
    },
    "start": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "status_package": {
      "type": "enumeration",
      "enum": [
        "Draft",
        "Publish"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "package"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location",
      "mappedBy": "package"
    },
    "note": {
      "type": "text"
    },
    "time": {
      "type": "time"
    },
    "max_people": {
      "type": "integer"
    },
    "meeting_point": {
      "type": "string"
    },
    "price_includes": {
      "type": "richtext"
    },
    "rating": {
      "type": "integer"
    },
    "description": {
      "type": "text"
    },
    "package_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::package-detail.package-detail",
      "mappedBy": "package"
    },
    "stripe_product_id": {
      "type": "string"
    },
    "stripe_price_id": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Nature & Mountain Tour",
        "Cultural & Historical Tour",
        "Adventure Tour",
        "Family Tour",
        "Honeymoon & Romantic Tour"
      ]
    }
  }
}
